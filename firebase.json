{
  "functions": {
    "predeploy": "npm --prefix \"$RESOURCE_DIR\" run build"
  },
  "hosting": [{
      "target": "server",
      "public": "luca-server/out"
    },
    {
      "target": "website",
      "public": "luca-site/build",
      "rewrites": [{
        "source": "**",
        "destination": "/index.html"
    }]
  }]
}
